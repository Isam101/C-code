#include <stdio.h>
#include <stdbool.h>
#include <string.h>

int main(){

 char names[3][25]={0}; // This program will ask the user to enter their names which will be stored in three rows and a maximum of 25 characters
for(int i=0; i<3;i++){
 printf("Please enter the first name: \n");
 fgets(names[i],sizeof(names[i]), stdin);
    names[i][strlen(names[i])-1]='\0';

    

}
for(int j=0;j<3; j++){
 printf("%s\n", names[j]);
    
}
    return 0;
}
