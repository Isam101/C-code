
#include <stdio.h>
#include <stdbool.h>

int main(){
int numbers[3][4]= {{1,2,3,4}, //How array indexing works (array[row][column])
                     {5,6,7,8},
                    {9,10,11,12}};

     // Calling a specific number in the 2d array the first [0] calls which set and then the second [0] calls the first number in the set 
     for(int i=0;i<3;i++){
        for(int j=0;j < 4; j++){
        printf("%d ",numbers[i][j]);
        }
        printf(("\n"));

     }
    


    return 0;
}
