  cff

#include <stdio.h>
#include <stdbool.h>

int main(){
int numbers[3][4]= {{1,2,3,4}, //How array indexing works (array[row][column])
                     {5,6,7,8},
                    {9,10,11,12}};

     // Calling a specific number in the 2d array the first [0] calls which set and then the second [0] calls the first number in the set 
     printf("%d ", numbers[0][0]);
     printf("%d ", numbers[0][1]);
     printf("%d ", numbers[0][2]);
     printf("%d \n", numbers[0][3]);

     printf("%d ",numbers[1][0]);
     printf("%d ", numbers[1][1]);
     printf("%d ", numbers[1][2]);
     printf("%d \n", numbers[1][3]);

     printf("%d ",numbers[2][0]);
     printf("%d ",numbers[2][1]);
     printf("%d ", numbers[2][2]);
     printf("%d ", numbers[2][3]);
    

    


    return 0;
}
