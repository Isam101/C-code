#include <stdio.h>

#include <stdbool.h>

int main(void){
    
    bool StudentisofAge;
    printf("Are you of age: For YES ENTER 1 AND FOR NO ENTER 0 \n");
    scanf("%i",&StudentisofAge);

    if(StudentisofAge == true){

        printf("I am allowed to go to the student trip with my friends in school");

    }

    else{

    printf("You are not of age try again next year");
    }




    return 0;
}


