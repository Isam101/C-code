#include <stdio.h>
#include <stdbool.h>


int main(){

int rows =0;
int columns =0;
char character = '\0';
printf("Please enter the number of rows: ");
scanf("%d",&rows);
printf("Please enter the number of columns: ");
scanf("%d",&columns);

printf("Please enter the character: ");
scanf(" %c",&character );

for(int j=0; j<rows; j++){

    for(int i=0 ;i <columns ; i++){

        printf("%c", character);
    }
    printf("\n");
}

return 0;


}
