#include <stdio.h>
#include <stdbool.h>

int main(){

    char Fruits[][10]={"Apple", "Orange","Cocunut"};
    int size= sizeof(Fruits)/sizeof(Fruits[0]); // To get the size the total number of bytes used by the entire array we use sizeof(fruits)then we divide with sizeof(Fruits[0])
                                                //This gives the size of one element of the array â€” in this case, the first string "Apple". which then gives us how many strings are in the array 

   


        for(int i=0; i<size;i++){

            printf("%s ",Fruits[i]);
        }

    return 0;
}
